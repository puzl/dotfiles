# do funcs first
RCS="bash_funcs bash_profile bash_aliases"

for rc in $RCS
do
    if [[ -f $HOME/.bash.d/$rc ]]; then
        source $HOME/.bash.d/$rc
    fi
done

#[[ "$OSTYPE" == "linux-gnu" && -f ~/.fzf.bash ]] && source ~/.fzf.bash
[[ -r "$HOME/.artifactoryrc" ]] && source "$HOME/.artifactoryrc"
